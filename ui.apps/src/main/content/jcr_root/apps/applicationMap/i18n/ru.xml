<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:language="ru"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="nt:folder">
    <copyright
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="welcome"
        sling:message="Добро пожаловать!"/>
    <title
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="titleMap"
            sling:message="Гид по Санкт-Петербургу"/>
    <pageInfo
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="pageDescription"
            sling:message="Эта страница сделана для предоставления краткого экскурса в историю и географию Санкт-Петербурга. Многие гости города (да что греха таить, порой и сами жители) не до конца представляют, где находится та или иная известная постройка и что она из себя представляет. Здесь вы можете найти на карте и бегло ознакомиться с основными достопримечательностями города"/>
    <close
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="close"
            sling:message="Закрыть"/>
    <save
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="save"
            sling:message="Сохранить"/>
    <delete
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="delete"
            sling:message="Удалить"/>
    <edit
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="edit"
            sling:message="Изменить"/>
    <name
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="name"
            sling:message="Описание события"/>
    <description
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="description"
            sling:message="Текст"/>
    <parameters
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="parameters"
            sling:message="Параметры события"/>
    <footer
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="footerText"
            sling:message="Санкт-Петербург 2018 г."/>
    <message0
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="message0"
            sling:message="Вы не ввели название"/>
    <message1
            jcr:mixinTypes="[sling:Message]"
            jcr:primaryType="nt:folder"
            sling:key="message1"
            sling:message="Вы не ввели описание"/>
</jcr:root>
